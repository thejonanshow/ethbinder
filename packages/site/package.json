{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "license": "(MIT-0 OR Apache-2.0)",
  "scripts": {
    "allow-scripts": "yarn workspace root allow-scripts",
    "build": "GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "clean": "rimraf public",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "start": "GATSBY_TELEMETRY_DISABLED=1 gatsby develop",
    "test:site": "cd site && yarn test",
    "test:site:mutation": "cd site && ../../scripts/stryker-helper.sh",
    "test:site:unit": "cd site && yarn test --testPathPattern=unit",
    "test:site:integration": "cd site && yarn test --testPathPattern=integration",
    "test:site:all": "yarn test:site:unit && yarn test:site:integration && yarn test:site:mutation && yarn test:site:e2e && yarn test:site:regression && yarn test:site:performance && yarn test:site:security && yarn test:site:a11y && yarn test:site:snapshot",
    "test:site:e2e": "cd site && yarn cypress run",
    "test:site:regression": "cd site && yarn visual-regression",
    "test:site:performance": "cd site && yarn lighthouse",
    "test:site:security": "cd site && yarn security-test",
    "test:site:a11y": "cd site && yarn axe-run",
    "test:site:smoke": "cd site && yarn test --testPathPattern=smoke",
    "test:site:snapshot": "cd site && yarn test --updateSnapshot",
    "test:unit": "jest --testMatch=\"**/__tests__/**/*.test.[jt]s?(x)\"",
    "test:integration": "jest --testMatch=\"**/__tests__/**/*.spec.[jt]s?(x)\"",
    "test:mutation": "jest --config=stryker.conf.json",
    "test:e2e": "cypress run",
    "test:all": "yarn test:unit && yarn test:integration && yarn test:mutation && yarn test:e2e"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@metamask/providers": "^16.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-github-login": "^1.0.3",
    "react-is": "^18.2.0",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@metamask/eslint-config": "^12.2.0",
    "@metamask/eslint-config-browser": "^12.1.0",
    "@metamask/eslint-config-jest": "^12.1.0",
    "@metamask/eslint-config-nodejs": "^12.1.0",
    "@metamask/eslint-config-typescript": "^12.1.0",
    "@svgr/webpack": "^6.4.0",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^41.1.2",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "gatsby": "^5.13.7",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-styled-components": "^6.13.1",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=18.6.0"
  }
}
